            "url": {
              "raw": "{{base_url}}/contracts/units/upload-csv/{{contract_id}}",
              "host": ["{{base_url}}"],
              "path": ["contracts", "units", "upload-csv", "{{contract_id}}"]
            },
            "description": "Bulk upload units via CSV. Form-data: csv_file (required). Permission: units.csv_upload"
          }
        },
        {
          "name": "Create Unit",
          "request": {
            "method": "POST",
            "header": [
              {"key": "Accept", "value": "application/json"},
              {"key": "Content-Type", "value": "application/json"}
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n    \"unit_type\": \"apartment\",\n    \"unit_number\": \"A-101\",\n    \"status\": \"available\",\n    \"price\": 500000,\n    \"area\": \"120 sqm\",\n    \"description\": \"3BR apartment\"\n}"
            },
            "url": {
              "raw": "{{base_url}}/contracts/units/store/{{contract_id}}",
              "host": ["{{base_url}}"],
              "path": ["contracts", "units", "store", "{{contract_id}}"]
            },
            "description": "Create a unit. Body: unit_type, unit_number (required), status (pending|sold|reserved|available), price (required), area, description. Permission: units.edit",
            "event": [
              {
                "listen": "test",
                "script": {
                  "exec": [
                    "if (pm.response.code === 201) {",
                    "    const jsonData = pm.response.json();",
                    "    if (jsonData.data && jsonData.data.id) {",
                    "        pm.environment.set('unit_id', jsonData.data.id);",
                    "    }",
                    "}"
                  ],
                  "type": "text/javascript"
                }
              }
            ]
          }
        },
        {
          "name": "Update Unit",
          "request": {
            "method": "PUT",
            "header": [
              {"key": "Accept", "value": "application/json"},
              {"key": "Content-Type", "value": "application/json"}
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n    \"status\": \"sold\",\n    \"price\": 520000\n}"
            },
            "url": {
              "raw": "{{base_url}}/contracts/units/update/{{unit_id}}",
              "host": ["{{base_url}}"],
              "path": ["contracts", "units", "update", "{{unit_id}}"]
            },
            "description": "Update a unit. Body: unit_type, unit_number, status, price, area, description (all optional). Permission: units.edit"
          }
        },
        {
          "name": "Delete Unit",
          "request": {
            "method": "DELETE",
            "header": [{"key": "Accept", "value": "application/json"}],
            "url": {
              "raw": "{{base_url}}/contracts/units/delete/{{unit_id}}",
              "host": ["{{base_url}}"],
              "path": ["contracts", "units", "delete", "{{unit_id}}"]
            },
            "description": "Delete a unit. Permission: units.edit"
          }
        }
      ]
    },
    {
      "name": "06 - Boards Department",
      "item": [
        {
          "name": "Get Boards (by contract)",
          "request": {
            "method": "GET",
            "header": [{"key": "Accept", "value": "application/json"}],
            "url": {
              "raw": "{{base_url}}/boards-department/show/{{contract_id}}",
              "host": ["{{base_url}}"],
